<h1>Add a Dog</h1>

<%= form_tag '/owners_index', method: :post do %>

  <div>
    <%= label_tag :name %>
    <%= text_field_tag :name%>
  </div>

  <div>
    <%= label_tag :breed %>
    <%= text_field_tag :breed %>
  </div>

  <div>
    <%= label_tag :gender %>
    <%= text_field_tag :gender%>
  </div>

 

  <%= submit_tag "Add Puppy" %>

 <% end %>